    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#InterestingPizza -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#InterestingPizza">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#Pizza"/>
                    <owl:Restriction>
                        <owl:onProperty rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#hasTopping"/>
                        <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">3</owl:minQualifiedCardinality>
                        <owl:onClass rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#PizzaTopping"/>
                    </owl:Restriction>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#JalapenoPepperTopping -->

    <owl:Class rdf:about="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#JalapenoPepperTopping">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#PepperTopping"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#hasSpiciness"/>
                <owl:hasValue rdf:resource="http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#Hot"/>
            </owl:Restriction>
        </rdfs:subClassOf>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/v0cn037/ontologies/2023/6/PizzaTutorial#MargheritaPizza -->

